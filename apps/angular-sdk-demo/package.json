{
  "name": "angular-sdk-demo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "pnpm run copy-sdk-dist && ng serve",
    "dev": "pnpm run copy-sdk-dist && ng serve",
    "build": "pnpm run copy-sdk-dist && ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "copy-sdk-dist": "rm -rf local-sdk && cpx \"../../packages/sdk/dist/**\" local-sdk"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.0",
    "@angular/common": "^15.2.0",
    "@angular/compiler": "^15.2.0",
    "@angular/core": "^15.2.0",
    "@angular/forms": "^15.2.0",
    "@angular/platform-browser": "^15.2.0",
    "@angular/platform-browser-dynamic": "^15.2.0",
    "@angular/router": "^15.2.0",
    "@demo/sdk": "workspace:^",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "cpx": "^1.5.0",
    "@angular-builders/custom-webpack": "^15.0.0",
    "@angular-devkit/build-angular": "^15.2.11",
    "@angular/cli": "~15",
    "@angular/compiler-cli": "^15.2.0",
    "@types/jasmine": "~4.3.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.9.4"
  }
}
